<?xml version="1.0"?>
<opencv_storage>

    <!-- 相机参数-->
    <exposure_auto>0</exposure_auto>
    <exposure_time>5000</exposure_time> <!-- us -->

    <w_auto>0</w_auto>
    <w_red>1.98</w_red>
    <w_green>1.0</w_green>
    <w_blue>1.98</w_blue>

    <gain_auto>0</gain_auto>
    <gain>15</gain>
    
    <!-- 图像参数-->
    <approx_epsilon>1.0</approx_epsilon>

    <blue_brightness_thresh>10</blue_brightness_thresh>
    <blue_color_thresh>10</blue_color_thresh>

    <red_brightness_thresh>20</red_brightness_thresh>
    <red_color_thresh>20</red_color_thresh>

    <blue_red_diff>20</blue_red_diff>

    <!-- 比赛 -->
    <!--内扇叶-->
    <!--21000-->
    <min_in_vane_area>17000</min_in_vane_area>
    <max_in_vane_area>25000</max_in_vane_area>
    
    <!--1.27-->
    <min_in_vane_ls_ratio>0.97</min_in_vane_ls_ratio>
    <max_in_vane_ls_ratio>1.57</max_in_vane_ls_ratio>

    <!--0.25-->
    <min_in_vane_contour_rrect_ratio>0.22</min_in_vane_contour_rrect_ratio>
    <max_in_vane_contour_rrect_ratio>0.28</max_in_vane_contour_rrect_ratio>

    <!--60-->
    <min_in_vane_hull_num>40</min_in_vane_hull_num>
    <max_in_vane_hull_num>75</max_in_vane_hull_num>

    <!--外扇叶-->
    <!--9500-->
    <min_out_vane_area>7500</min_out_vane_area>
    <max_out_vane_area>11500</max_out_vane_area>

    <!--2.2-->
    <min_out_vane_ls_ratio>1.9</min_out_vane_ls_ratio>
    <max_out_vane_ls_ratio>2.5</max_out_vane_ls_ratio>

    <!--0.4-->
    <min_out_vane_contour_rrect_ratio>0.25</min_out_vane_contour_rrect_ratio>
    <max_out_vane_contour_rrect_ratio>0.55</max_out_vane_contour_rrect_ratio>

    <!--内外扇叶-->
    <!--1-->
    <min_distance_inlongside_ratio>0.7</min_distance_inlongside_ratio>
    <max_distance_inlongside_ratio>1.3</max_distance_inlongside_ratio>

    <max_in_out_vane_derta_angle>7</max_in_out_vane_derta_angle>

    <!--圆心-->
    <min_R_in_area_ratio>20</min_R_in_area_ratio>
    <max_R_in_area_ratio>20</max_R_in_area_ratio>
    <min_R_to_predict_distance_ratio>0.0</min_R_to_predict_distance_ratio>
    <max_R_to_predict_distance_ratio>0.3</max_R_to_predict_distance_ratio>
    
    <RtoOut_RtoIn>1.95</RtoOut_RtoIn>

    <max_diff_distance_ratio>0.5</max_diff_distance_ratio>

    <!-- Kalman参数-->

    <kalman_spd_Q type_id="opencv-matrix">  <!-- Q 过程噪声-->
    <rows>2</rows>
    <cols>2</cols>
    <dt>f</dt>
    <data>
        500000. 0.   <!-- 1-->
        0. 500000.
    </data>
        
    </kalman_spd_Q>

    <!--值越大,变化趋势越趋于稳定-->
    <kalman_spd_R type_id="opencv-matrix">  <!-- R 测量噪声(角度angle)-->
    <rows>1</rows>
    <cols>1</cols>
    <dt>f</dt>
    <data>
       0.09

    </data>
    </kalman_spd_R>

    <!-- EKF参数-->

    <EKF_spd_Q type_id="opencv-matrix">  <!-- Q 过程噪声-->
    <rows>2</rows>
    <cols>2</cols>
    <dt>f</dt>
    <data>
        500000. 0.   <!-- 1-->
        0. 500000.
    </data>
        
    </EKF_spd_Q>

    <!--值越大,变化趋势越趋于稳定-->
    <EKF_spd_R type_id="opencv-matrix">  <!-- R 测量噪声(角度angle)-->
    <rows>1</rows>
    <cols>1</cols>
    <dt>f</dt>
    <data>
       0.09

    </data>
    </EKF_spd_R>

</opencv_storage>